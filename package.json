{
	"name": "botified.chat",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"cf:dev": "pnpx wrangler dev --remote",
		"db:gen:sql": "drizzle-kit generate:sqlite",
		"db:up:sql": "drizzle-kit up:sqlite",
		"cf:mig:sql": "wrangler d1 execute botified-dev-db --remote",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240405.0",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/adapter-cloudflare-workers": "^2.3.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/eslint": "^8.56.9",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.37.0",
		"globals": "^15.0.0",
		"lucia": "^3.1.1",
		"postcss": "^8.4.38",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"svelte": "^4.2.14",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.6.0",
		"vite": "^5.2.8",
		"wrangler": "^3.50.0"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.2",
		"@langchain/cloudflare": "^0.0.4",
		"@langchain/community": "^0.0.48",
		"@langchain/core": "0.1.57",
		"bits-ui": "^0.21.3",
		"clsx": "^2.1.0",
		"cmdk-sv": "^0.0.17",
		"drizzle-kit": "^0.20.14",
		"drizzle-orm": "^0.30.8",
		"embla-carousel-svelte": "^8.0.2",
		"formsnap": "^1.0.0",
		"langchain": "^0.1.33",
		"lucide-svelte": "^0.368.0",
		"mode-watcher": "^0.3.0",
		"oslo": "^1.2.0",
		"paneforge": "^0.0.4",
		"svelte-sonner": "^0.3.22",
		"sveltekit-superforms": "^2.12.4",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"vaul-svelte": "^0.3.0",
		"zod": "^3.22.4"
	},
	"overrides": {
		"@langchain/core": "0.1.57"
	},
	"pnpm": {
		"overrides": {
			"@langchain/core": "0.1.57"
		}
	}
}
